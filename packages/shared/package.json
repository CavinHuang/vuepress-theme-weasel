{
  "name": "@mr-huang/vuepress-shared",
  "version": "0.0.1",
  "description": "weasel theme",
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src",
    "styles"
  ],
  "author": "mr-huang",
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm clean\" \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "cpx src/client/**/*.{vue,scss} lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "homepage": "https://cavinHuang.github.io/vuepress-theme-weasel",
  "repository": {
    "type": "git",
    "url": "https://github.com/cavinHuang/vuepress-theme-weasel"
  },
  "bugs": {
    "url": "https://github.com/cavinHuang/vuepress-theme-weasel/issues"
  },
  "keywords": [
    "vue3",
    "weasel",
    "vuepress plugin",
    "rollup",
    "vuepress"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "devDependencies": {
    "@vuepress/bundler-vite": "^2.0.0-beta.35",
    "@vuepress/core": "^2.0.0-beta.35"
  },
  "dependencies": {
    "@mr-huang/vuepress-theme-weasel": "workspace:^0.0.1",
    "@mr-huang/weasel-theme-demo": "workspace:^0.0.1",
    "@vuepress/shared": "^2.0.0-beta.35",
    "balloon-css": "^1.2.0",
    "chalk": "4.1.2",
    "dayjs": "^1.10.7",
    "ora": "5.4.1",
    "org": "^0.2.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.12"
  }
