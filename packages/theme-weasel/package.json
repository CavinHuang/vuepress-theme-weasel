{
  "name": "@mr-huang/vuepress-theme-weasel",
  "version": "0.0.1",
  "description": "weasel theme",
  "main": "lib/node/index.js",
  "typings": "lib/node/index.d.ts",
  "author": "mr-huang",
  "scripts": {
    "build": "pnpm build:ts && pnpm build:copy",
    "build:copy": "cpx src/client/**/*.{css,scss,jpg,vue} lib/client",
    "build:ts": "tsc -b tsconfig.release.json",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm clean\" \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "yarn build:copy -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch"
  },
  "files": [
    "dist",
    "typings",
    "README"
  ],
  "homepage": "https://cavinHuang.github.io/vuepress-theme-weasel",
  "repository": {
    "type": "git",
    "url": "https://github.com/cavinHuang/vuepress-theme-weasel"
  },
  "bugs": {
    "url": "https://github.com/cavinHuang/vuepress-theme-weasel/issues"
  },
  "keywords": [
    "vue3",
    "weasel",
    "vuepress plugin",
    "rollup",
    "vuepress"
  ],
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@mr-huang/vuepress-plugin-sass-palette": "workspace:^0.0.1",
    "@mr-huang/vuepress-theme-weasel": "workspace:^0.0.1",
    "@vuepress/client": "^2.0.0-beta.35",
    "@vuepress/core": "^2.0.0-beta.35",
    "@vuepress/plugin-active-header-links": "^2.0.0-beta.35",
    "@vuepress/plugin-container": "^2.0.0-beta.35",
    "@vuepress/plugin-external-link-icon": "^2.0.0-beta.35",
    "@vuepress/plugin-git": "^2.0.0-beta.35",
    "@vuepress/plugin-theme-data": "^2.0.0-beta.35",
    "@vuepress/utils": "^2.0.0-beta.35",
    "balloon-css": "^1.2.0",
    "vue": "^3.2.29"
  }
}
