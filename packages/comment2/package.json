{
  "name": "@mr-huang/vuepress-plugin-comment",
  "version": "0.0.1",
  "description": "A comment plugin for vuepress",
  "keywords": [
    "vuepress-plugin",
    "vuepress-blog",
    "comment",
    "valine",
    "vssue",
    "page-info"
  ],
  "homepage": "https://vuepress-theme-weasel.github.io/v2/comment/",
  "bugs": {
    "url": "https://github.com/vuepress-theme-weasel/vuepress-theme-weasel/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuepress-theme-weasel/vuepress-theme-weasel.git",
    "directory": "packages/comment"
  },
  "license": "MIT",
  "author": {
    "name": "Mr.weasel",
    "email": "mister-weasel@outlook.com",
    "url": "https://mrweasel.site"
  },
  "main": "lib/node/index.js",
  "types": "lib/node/index.d.ts",
  "files": [
    "lib",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "clean": "rimraf ./lib ./*.tsbuildinfo",
    "dev": "concurrently \"pnpm dev:copy\" \"pnpm dev:ts\"",
    "dev:copy": "cpx \"src/client/**/*.{vue,scss}\" lib/client -w",
    "dev:ts": "tsc -b tsconfig.build.json --watch",
    "dev:demo": "vuepress-vite dev __tests__/demo",
    "build:demo": "vuepress-vite build __tests__/demo"
  },
  "dependencies": {
    "@giscus/vue": "^1.1.1",
    "@mr-huang/vuepress-shared": "workspace:^0.0.1",
    "@vuepress/client": "2.0.0-beta.38",
    "@vuepress/utils": "2.0.0-beta.38",
    "@waline/client": "^1.6.0",
    "twikoo": "^1.5.0",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14"
  },
  "devDependencies": {
    "@vuepress/core": "2.0.0-beta.38"
  },
  "publishConfig": {
    "access": "public"
  }
}
