{
  "name": "vuepress-theme-weasel-demo",
  "version": "0.0.1",
  "description": "A vuepress theme and tools",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev:libs": "concurrently --kill-others \"pnpm dev -C packages/shared\"",
    "dev:theme": "pnpm run dev -C packages/theme",
    "dev:demo": "pnpm run dev -C demo",
    "create:pkg": "plop",
    "clean": "rimraf ./packages/**/lib ./packages/**/*.tsbuildinfo",
    "commit": "git-cz",
    "release": "standard-version",
    "lint": "eslint ./packages --ext .vue,.js,.ts",
    "lint-fix": "eslint --fix ./packages --ext .vue,.js,.ts",
    "prettier": "prettier --write ./packages",
    "clean:node": "rimraf ./packages/**/node_modules && rimraf ./node_modules",
    "reset": "pnpm run clean:node && pnpm run clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CavinHuang/vuepress-theme-weasel.git"
  },
  "keywords": [
    "weasel",
    "theme",
    "vuepress"
  ],
  "author": "cavinHuang",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/CavinHuang/vuepress-theme-weasel/issues"
  },
  "homepage": "https://github.com/CavinHuang/vuepress-theme-weasel#readme",
  "dependencies": {
    "@mr-huang/vuepress-shared": "workspace:^0.0.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@vuepress/core": "^2.0.0-beta.37",
    "@vuepress/utils": "^2.0.0-beta.37",
    "chalk": "4.1.2",
    "esno": "^0.14.1",
    "ora": "5.4.1",
    "plop": "^3.0.5"
  }
}
